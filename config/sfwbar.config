{
  "placement": {
    "xcascade": 5,
    "ycascade": 5,
  },
  "scanner": [
  {
    "name":"/proc/net/dev", "variables": [
    {"number":"EthRcvd","value":"eth[\\t 0-9]*:[\\t ]*([0-9]+)","flag":"Add"},
    {"number":"EthSent","value":"eth[\\t 0-9]*:(?:[\\t ]*[0-9]+){8}[\\t ]([0-9]+)","flag":"Add"}]
  },
  {
    "name":"/proc/meminfo", "variables": [
    {"number":"MemTotal","value":"^MemTotal:[\\t ]*([0-9]+)[\\t ]"},
    {"number":"MemFree","value":"^MemFree:[\\t ]*([0-9]+)[\\t ]"},
    {"number":"MemCache","value":"^Cached:[\\t ]*([0-9]+)[\\t ]"},
    {"number":"MemBuff","value":"^Buffers:[\\t ]*([0-9]+)[\\t ]"}]
  },
  {
    "name":"/proc/swaps", "variables": [
    {"number":"SwapTotal","value":"[\\t ]([0-9]+)","flag":"Add"},
    {"number":"SwapUsed","value":"[\\t ][0-9]+[\\t ]([0-9]+)","flag":"Add"}]
  },
  {
    "name":"/proc/stat", "variables": [
    {"number":"CpuUser","value":"^cpu [\\t ]*([0-9]+)","flag":"Add"},
    {"number":"CpuNice","value":"^cpu [\\t ]*[0-9]+ ([0-9]+)","flag":"Add"},
    {"number":"CpuSystem","value":"^cpu [\\t ]*[0-9]+ [0-9]+ ([0-9]+)","flag":"Add"},
    {"number":"CpuIdle","value":"^cpu [\\t ]*[0-9]+ [0-9]+ [0-9]+ ([0-9]+)","flag":"Add"}]
  },
  {
    "name":"/sys/class/hwmon/hwmon2/temp1_input", "variables": [
    {"number":"MBTemp","value":"([0-9]+)"}]
  },
  {
    "name":"/sys/class/hwmon/hwmon1/temp1_input", "variables": [
    {"number":"CPUTemp","value":"([0-9]+)"}]
  },
  {
    "name":"/sys/devices/platform/it87*/hwmon/hwmon2/fan2_input", "variables": [
    {"number":"CPUFan","value":"([0-9]+)"}]
  },
  {
    "name":"cat /home/crion/dev/sfwbar/src/*", "flags":"Exec", "variables": [
    {"number":"Test","value":"UP_UPDATE = ([0-9])"}]
  },
  {
    "name":"/sys/devices/platform/it87*/hwmon/hwmon2/fan1_input", "variables": [
    {"number":"MBFan","value":"([0-9]+)"}]
  },
  {
    "name":"wget -O - \"https://api.met.no/weatherapi/locationforecast/2.0/compact?lat=51.8711&lon=0.1587\"", "flags":"Exec", "variables": [
    {"string":"WthrTemp","value":"\"timeseries\".*\"air_temperature\":([0-9\\.]*)"},
    {"number":"WthrPrss","value":"\"timeseries\".*\"air_pressure_at_sea_level\":([0-9\\.]*)"},
    {"string":"WthrWind","value":"\"timeseries\".*\"wind_speed\":([0-9\\.]*)"},
    {"string":"WthrCond","value":"\"timeseries\".*\"symbol_code\":\"([A-Za-z_]*)\""}]
  } ],
  
  "layout": [
  {
    "type": "taskbar",
    "expand": true,
    "icon": true,
    "title": true,
    "isize": 16,
    "rows": 2
  },
  {
    "type":"label",
    "expand":false
  },
  {
    "type":"pager",
    "rows":2
  },
  {
    "type": "button",
    "action": "firefox",
    "icon": "firefox",
    "isize": 48
  },
  {
    "type": "button",
    "action": "alacritty",
    "icon": "Alacritty",
    "isize": 48
  },
  {
    "type": "grid",
    "children": [
    { "type": "scale", "style":"blue", "direction": "vertical", "name": "CPU",
      "value": "(CpuUser-CpuUser.pval)/(CpuUser+CpuNice+CpuSystem+CpuIdle-CpuUser.pval-CpuNice.pval-CpuSystem.pval-CpuIdle.pval)" },
    { "type": "scale", "style":"green", "direction": "vertical", "name": "Mem",
      "value": "(MemTotal-MemFree-MemCache-MemBuff)/MemTotal" },
    { "type": "scale", "style":"red", "direction": "vertical", "name": "Swap",
      "value": "SwapUsed/SwapTotal" } ]
  },
  {
    "type": "grid",
    "style": "frame",
    "children": [
    { "type": "label", "style": "static", "value": "'ETH:'", "x": 1, "y": 1, "xalign":0 },
    { "type": "label", "style": "static", "value": "'CPU:'", "x": 1, "y": 2, "xalign":0  },
    { "type": "label", "style": "static", "value": "'M/B:'", "x": 1, "y": 3, "xalign":0  },
    { "type": "label", "style": "static", "value": "'FAN:'", "x": 1, "y": 4, "xalign":0  },
    { "type": "label", "style": "value", 
      "value": "Str((EthRcvd-EthRcvd.pval)/0.001024/EthRcvd.time,0)+'K/s'", "x": 2, "y": 1, "xalign": 1},
    { "type": "label", "style": "value", "value": "Str(CPUTemp/1000,1)", "x": 2, "y": 2, "xalign": 1 },
    { "type": "label", "style": "value", "value": "Str(MBTemp/1000,1)", "x": 2, "y": 3, "xalign": 1 },
    { "type": "label", "style": "value", "value": "Str(CPUFan,0)", "x": 2, "y": 4, "xalign": 1 }]
  },
  {
    "type": "grid",
    "style": "frame",
    "children": [
    { "type": "label", "freq":300000, "style": "bold", "value": "$WthrTemp", "x":1, "y":1, "w":4, "h":1, "xalign":0.5 },
    { "type": "label", "freq":300000, "style": "", "value": "$WthrCond", "x":1, "y":2, "w":4, "h":1, "xalign":0.5 },
    { "type": "label", "freq":300000, "style": "w10", "value": "'W:'", "x":1, "y":3, "xalign":0 },
    { "type": "label", "freq":300000, "style": "w25", "value": "$WthrWind", "x":2, "y":3, "xalign":0 },
    { "type": "label", "freq":300000, "style": "w10", "value": "'P:'", "x":3, "y":3, "xalign":0 },
    { "type": "label", "freq":300000, "style": "w25", "value": "$WthrPrss", "x":4, "y":3, "xalign":0 } ]
  },
  {
    "type": "grid",
    "style": "frame",
    "children": [
    { "type": "label", "style": "bold", 
      "value": "Extract(Time(),'([0-9]*:[0-9]*)')", "x":1, "y":1, "xalign":0.5 },
    { "type": "label", "style": "", 
      "value": "Extract(Time(),'^([A-Za-z]+\\ [A-Za-z]+[\\ ]+[0-9]+)')", "x":1, "y":2, "xalign":0.5 } ]
  } ]
} 
    
